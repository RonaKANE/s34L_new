<h1>Listing Users</h1>
<%= link_to 'New user', new_user_path %>
<table>
  <tr>
    <th>User Id</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Service</th>
    <th>Tel</th>
    <th>Mail</th>
    <th>Office</th>
    <th>Certificate's Name</th>
    <th>Password</th>
    <th colspan="5"></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
    <td><%= user.user_id %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.service %></td>
    <td><%= user.user_tel %></td>
    <td><%= user.user_mail %></td>
    <td><%= user.office %></td>
     <td><%= user.certificate_name %></td>
    <td><%= user.password %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %></td>
      <td><%= link_to 'Destroy', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Back to home page', root_path %>
